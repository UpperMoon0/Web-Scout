# Web Scout MCP Server Configuration

# Search API Keys (Optional - for enhanced search functionality)
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CSE_ID=your_custom_search_engine_id_here
BING_API_KEY=your_bing_search_api_key_here

# Server Configuration
WEB_SCOUT_USER_AGENT=Web-Scout-MCP/0.1.0
WEB_SCOUT_MAX_RETRIES=3
WEB_SCOUT_TIMEOUT=30
WEB_SCOUT_HEADLESS=true
WEB_SCOUT_CACHE_DIR=.web_scout_cache
WEB_SCOUT_CACHE_TTL=3600
WEB_SCOUT_RATE_LIMIT=60
WEB_SCOUT_MAX_CONTENT=1048576

# Environment and Logging
WEB_SCOUT_ENV=production
WEB_SCOUT_LOG_LEVEL=INFO
WEB_SCOUT_MONITORING=true

# Optional: OpenAI API for enhanced content analysis
OPENAI_API_KEY=your_openai_api_key_here